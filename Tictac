 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 		case 2:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 	        break;
 		case 3:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 		case 4:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 		case 6:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 	       }//inner switch ends.......
   	   }


     }//if b9-7 ends here.........

  //_________________________________________________________________________|
  //-------------------------- i9 starts and i7 ends-------------------------|
  //_________________________________________________________________________|

  if(i==9)
    {

      if(ar[2]==1 && ar[4]==8)
   	    {
   	     switch(ar[z])
 	       {
 	   	case 3:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 4:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 		case 6:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 	       }//inner switch ends.......
   	   }

  //--------------------------------case9, i9-------------------------------------------

   if(ar[2]==2  && ar[4]==4)
   	     {
   	      switch(ar[z])
 	       {
 	   	case 1:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 		case 3:
 		    b1.setLabel("O"); b1.setEnabled(false); ++z; ar[z]=1;
 		break;
 		case 8:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 	       }//inner switch ends.......
   	    }

//-----------------------------------case9, i9-----------------------------------

   	if(ar[2]==3  && ar[4]==4)
   	   {
   	   switch(ar[z])
 	       {
 	   	case 1:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 7:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 8:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 	       }//inner switch ends.......
   	   }

  //---------------------------------case9, i9----------------------------------------

   	if(ar[2]==7  && ar[4]==2)
   	   {
   	  switch(ar[z])
 	       {
 	        case 1:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 		break;
 		case 3:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 		break;
 		case 4:
 		    b1.setLabel("O"); b1.setEnabled(false); ++z; ar[z]=1;
 		break;
 	       }//inner switch ends.......
   	   }
    //----------------------------case 9, i=9----------------------------------------
    if(ar[2]==4 && ar[4]==2)
   	{
        switch(ar[z])
 	       {
 	   	case 1:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 6:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 7:
 		    b1.setLabel("O"); b1.setEnabled(false); ++z; ar[z]=1;
 		break;
 	       }//inner switch ends.......
   	   }

 //-------------------------------case9, i9-----------------------------------------

    if(ar[2]==6 && ar[4]==7)
   	{
        switch(ar[z])
 	       {
 	   	case 1:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 		case 2:
 		    b1.setLabel("O"); b1.setEnabled(false); ++z; ar[z]=1;
 		break;
 		case 4:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 	       }//inner switch ends.......
   	   }

 //---------------------------------case9, i9-------------------------------------------
    if(ar[2]==8 && ar[4]==3)
   	{
        switch(ar[z])
 	       {
 	   	case 1:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 		break;
 		case 2:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 		break;
 		case 4:
 		    b1.setLabel("O"); b1.setEnabled(false); ++z; ar[z]=1;
 		break;
 	       }//inner switch ends.......
   	   }
        }//i9 ends here

     break;  //case-9 ends here......
 /*
 |___________________________________________casesss ends here_________________________________________________________|
 |--------------------------------------------------------------------------------------------------------------------|
 |________________________________________computer will not play now__________________________________________|
 |--------------------------------------------------------------------------------------------------------------------|
 */


       }//switch ends here...............

     ++z;
              u=1;
              t.strt(true);

   }//sense ends here.................

/*
 |___________________________________________arti-sense ends here______________________________________________________|
 |--------------------------------------------------------------------------------------------------------------------|

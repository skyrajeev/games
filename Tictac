 public void actionPerformed(ActionEvent ae)
 {
   i=i+1;

	if(ae.getSource()==b1)
   {
	   b1.setLabel(ss);
	   b1.setEnabled(false);
	   ++i;
                       
                         t.strt(false);
	   sense(1);
   }

   if(ae.getSource()==b2)
      {
   	   b2.setLabel(ss);
   	   b2.setEnabled(false);
   	   ++i;
                       t.strt(false);
   	   sense(2);
   }

   if(ae.getSource()==b3)
      {
   	   b3.setLabel(ss);
   	   b3.setEnabled(false);
   	   ++i;
              t.strt(false);
   	   sense(3);
   }

   if(ae.getSource()==b4)
      {
   	   b4.setLabel(ss);
   	   b4.setEnabled(false);
   	   ++i; t.strt(false);
   	   sense(4);
   }

   if(ae.getSource()==b5)
      {
   	   b5.setLabel(ss);
   	   b5.setEnabled(false);
   	   i=i+1; t.strt(false);
   	   sense(5);
   }

   if(ae.getSource()==b6)
      {
   	   b6.setLabel(ss);
   	   b6.setEnabled(false);
   	   ++i; t.strt(false);
   	   sense(6);
   }

   if(ae.getSource()==b7)
      {
   	   b7.setLabel(ss);
   	   b7.setEnabled(false);
   	   ++i; t.strt(false);
   	   sense(7);
   }

   if(ae.getSource()==b8)
      {
   	   b8.setLabel(ss);
   	   b8.setEnabled(false);
   	   ++i; t.strt(false);
   	   sense(8);
   }

   if(ae.getSource()==b9)
      {
   	   b9.setLabel(ss);
   	   b9.setEnabled(false);
   	   ++i; t.strt(false);
   	   sense(9);
   }

   if(ae.getSource()==exit1)
   { System.exit(0); }

    if(ae.getSource()==exit2)
   { //System.exit(0);
        p2.setVisible(false);
        p3.setVisible(true);
        i=0;u=0;
    }

    if(ae.getSource()==exit3)
   { System.exit(0); }


   if(ae.getSource()==start)
   {
	   user1=tf1.getText();
	   user2=tf2.getText();
	   p1.setVisible(false);
	   p2.setVisible(true);
               th.start();
	}

  if(ae.getSource()==see)
   {

	   p3.setVisible(false);
	   p2.setVisible(true);
           b1.setEnabled(false);
           b2.setEnabled(false);
           b3.setEnabled(false);
           b4.setEnabled(false);
           b5.setEnabled(false);
           b6.setEnabled(false);
           b7.setEnabled(false);
           b8.setEnabled(false);
           b9.setEnabled(false);

           for(int g=0;g<9 && ar[g]!=0 ;g++)
           {
               int gg=g+1;  // System.out.println(ar[g]);

               switch(ar[g])
                 {
                 case 1:
                     if(g%2==0)
                     b1.setLabel("X"+String.valueOf(gg));
                     else
                     b1.setLabel("O"+String.valueOf(gg));
                     break;
                case 2:
                     if(g%2==0)
                     b2.setLabel("X"+String.valueOf(gg));
                     else
                     b2.setLabel("O"+String.valueOf(gg));
                     break;

                case 3:
                     if(g%2==0)
                     b3.setLabel("X"+String.valueOf(gg));
                     else
                     b3.setLabel("O"+String.valueOf(gg));
                     break;

               case 4:
                     //System.out.println("in 4");
                     if(g%2==0)
                     b4.setLabel("X"+String.valueOf(gg));
                     else
                     b4.setLabel("O"+String.valueOf(gg));
                     break;

               case 5:
                     if(g%2==0)
                     b5.setLabel("X"+String.valueOf(gg));
                     else
                     b5.setLabel("O"+String.valueOf(gg));
                     break;

              case 6:
                     if(g%2==0)
                     b6.setLabel("X"+String.valueOf(gg));
                     else
                     b6.setLabel("O"+String.valueOf(gg));
                     break;

             case 7:
                     if(g%2==0)
                     b7.setLabel("X"+String.valueOf(gg));
                     else
                     b7.setLabel("O"+String.valueOf(gg));
                     break;

             case 8:
                     if(g%2==0)
                     b8.setLabel("X"+String.valueOf(gg));
                     else
                     b8.setLabel("O"+String.valueOf(gg));
                     break;

            case 9:
                     if(g%2==0)
                     b9.setLabel("X"+String.valueOf(gg));
                     else
                     b9.setLabel("O"+String.valueOf(gg));
                     break;
             }



           }//for ends

  } //see ends


 if(ae.getSource()==replay)
   {
               
	   p3.setVisible(false);
	   p2.setVisible(true);
           b1.setEnabled(true);b1.setLabel("");
           b2.setEnabled(true);b2.setLabel("");
           b3.setEnabled(true);b3.setLabel("");
           b4.setEnabled(true);b4.setLabel("");
           b5.setEnabled(true);b5.setLabel("");
           b6.setEnabled(true);b6.setLabel("");
           b7.setEnabled(true);b7.setLabel("");
           b8.setEnabled(true);b8.setLabel("");
           b9.setEnabled(true);b9.setLabel("");
           i=1;z=0;
           for(int h=0;h<9;h++)
               ar[h]=0;
              u=1;
              t.strt(true);
           //Thread thl=new Thread(t);
         // thl.start();
  }


/*
 |___________________________________________Buttons action Ends here_________________________________________________|
 |--------------------------------------------------------------------------------------------------------------------|
 |________________________________________Win or loose conditions starts here_________________________________________|
 |--------------------------------------------------------------------------------------------------------------------|
 */


if(i>=5)
   {
	   if(i==11)
	      {
			  p2.setVisible(false);
		   p3.setVisible(true);
			  ll.setText(" This Game is Drawn:  ");
                                              t.strt(false);
		 }
	   s1=b1.getLabel(); s2=b2.getLabel(); s3=b3.getLabel(); s4=b4.getLabel(); s5=b5.getLabel(); s6=b6.getLabel(); s7=b7.getLabel(); s8=b8.getLabel(); s9=b9.getLabel();

	   if((s1.equals("X") && s2.equals("X") && s3.equals("X")) || (s1.equals("O") && s2.equals("O") && s3.equals("O")))
	   { 
		   p2.setVisible(false);
		   p3.setVisible(true);
		   if(s1.equals("X"))
		      ll.setText(user1+" You Won ");
		   else
		     ll.setText(user2+" You Won ");
                    t.strt(false);
	   }

	   if((s4.equals("X") && s5.equals("X") && s6.equals("X")) || (s4.equals("O") && s5.equals("O") && s6.equals("O")))
	   	   { 
                                                                           p2.setVisible(false);
		  		   p3.setVisible(true);
		  		   if(s4.equals("X"))
		  		      ll.setText(user1+" You Won ");
		  		   else
		              ll.setText(user2+" You Won "); t.strt(false);
	   }

	   if((s7.equals("X") && s8.equals("X") && s9.equals("X")) || (s7.equals("O") && s8.equals("O") && s9.equals("O")))
	   	   {
                                                                              p2.setVisible(false);
			  		   p3.setVisible(true);
			  		   if(s7.equals("X"))
			  		      ll.setText(user1+" You Won ");
			  		   else
		                   ll.setText(user2+" You Won "); t.strt(false);
	   }

//____________________________________________________________________________________________________________________|
//--------------------------------------------------------------------------------------------------------------------|
	   if((s1.equals("X") && s4.equals("X") && s7.equals("X")) || (s1.equals("O") && s4.equals("O") && s7.equals("O")))
	   	   { 
                                                                        p2.setVisible(false);
			 		   p3.setVisible(true);
			 		   if(s1.equals("X"))
			 		      ll.setText(user1+" You Won ");
			 		   else
		     ll.setText(user2+" You Won "); t.strt(false);
	   }

	   if((s2.equals("X") && s5.equals("X") && s8.equals("X")) || (s2.equals("O") && s5.equals("O") && s8.equals("O")))
	   	   { 
                                                                      p2.setVisible(false);
			 		   p3.setVisible(true);
			 		   if(s2.equals("X"))
			 		      ll.setText(user1+" You Won ");
			 		   else
		     ll.setText(user2+" You Won "); t.strt(false);
	   }

	   if((s3.equals("X") && s6.equals("X") && s9.equals("X")) || (s3.equals("O") && s6.equals("O") && s9.equals("O")))
	   	   {
                                                                      p2.setVisible(false);
			  		   p3.setVisible(true);
			  		   if(s3.equals("X"))
			  		      ll.setText(user1+" You Won ");
			  		   else
		     ll.setText(user2+" You Won "); t.strt(false);
	   }

//______________________________________________________________________________________________________________________|
//----------------------------------------------------------------------------------------------------------------------|

	   if((s1.equals("X") && s5.equals("X") && s9.equals("X")) || (s1.equals("O") && s5.equals("O") && s9.equals("O")))
	   	   { 
                                                                        p2.setVisible(false);
					   p3.setVisible(true);
					   if(s1.equals("X"))
					      ll.setText(user1+" You Won ");
					   else
		                                                               ll.setText(user2+" You Won ");
                                                                                  t.strt(false);
	                                   }

	   if((s3.equals("X") && s5.equals("X") && s7.equals("X")) || (s3.equals("O") && s5.equals("O") && s7.equals("O")))
	   	   { cond=false;
                                                                           p2.setVisible(false);
				   p3.setVisible(true);
				  if(s3.equals("X"))
			                      ll.setText(user1+" You Won ");
				 else
		                                          ll.setText(user2+" You Won ");
                                                                 t.strt(false);
	                           }

   }//win or loose ends here---------------

     

      
 }//action ends here..........................................................................


/*

 public void sense(int zz)
 {
    // try{Thread.sleep(100);}catch(InterruptedException ie){System.out.println(ie);}

 	ar[z]=zz;
        //System.out.println(ar[z]);
 	//System.out.println(i);

    switch(ar[0])
      {
         case 5:

            if(i==3)
                 {
 	          b9.setLabel("O"); b9.setEnabled(false); ++z; ar[z]=9;
                  }
       //_________________________________________________________________________|
       //----------------------case5, i=5 starts----------------------------------|
       //_________________________________________________________________________|

           if(i==5)
   	      {
              switch(ar[z])
 	        {
 	   	 case 1:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z;ar[z]=3;
 		 break;
 		 case 3:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		 break;
 		 case 7:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		 break;
 		 case 2:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		 break;
 		 case 4:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		 break;
 		 case 6:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 		 break;
 		 case 8:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 	     	 break;

 		}//inner switch ends.......

             }//if=5..i=5 ends here.........

       //_________________________________________________________________________|
       //-------------------------- case5, i=7 starts-----------------------------|
       //_________________________________________________________________________|
          if(i==7)
             {
             if(ar[2]==1)
   	       {
                switch(ar[z])
 	        {
 	   	 case 2:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
                 break;
 		case 6:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 	        break;
 		case 4:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 	        break;
 		case 7:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 8:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 	       }//inner switch ends.......
   	     }

   //-------------------------------------case5---------------------------------------

         if(ar[2]==2)
   	      {
              switch(ar[z])
 	       {
 	   	case 1:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 	        break;
 		case 3:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 4:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 	        break;
 		case 6:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 7:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 	        }//inner switch ends.......
   	      }

     //-------------------------------case5,i7------------------------------------------
   	  if(ar[2]==3)
   	      {
   	      switch(ar[z])
 	       {
 	        case 1:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 2:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 	        break;
 		case 4:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 6:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 8:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 	        break;
 	      }//inner switch ends.......
   	   }

    //-----------------------------------case5,i7-----------------------------------------

   	if(ar[2]==4)
   	     {
   	   switch(ar[z])
 	       {
 	        case 1:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 	        break;
 		case 2:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 	        break;
 		case 3:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 7:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 		case 8:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 	        break;
 	      }//inner switch ends.......
   	    }
    //---------------------------------case5,i7------------------------------------
       if(ar[2]==7)
   	     {
   	   switch(ar[z])
 	       {
 	        case 1:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 2:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 4:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 6:
 		    b4.setLabel("O"); b4.setEnabled(false); ++z; ar[z]=4;
 	        break;
 		case 8:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 	        break;
 	      }//inner switch ends.......
   	    }

     //-------------------------------case5,i7-----------------------------------------
       if(ar[2]==6)
   	     {
   	 switch(ar[z])
 	       {
 	        case 1:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 2:
 		    b8.setLabel("O"); b8.setEnabled(false); ++z; ar[z]=8;
 		break;
 		case 3:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 		break;
 		case 7:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 		case 8:
 		    b2.setLabel("O"); b2.setEnabled(false); ++z; ar[z]=2;
 		break;
 	      }//inner switch ends.......
   	    }
   //--------------------------------case5,i7----------------------------------------

       if(ar[2]==8)
   	   {
   	  switch(ar[z])
 	       {
 	   	case 1:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;
 		break;
 		case 3:
 		    b7.setLabel("O"); b7.setEnabled(false); ++z; ar[z]=7;
 	        break;
 		case 4:
 		    b6.setLabel("O"); b6.setEnabled(false); ++z; ar[z]=6;
 		break;
 		case 7:
 		    b3.setLabel("O"); b3.setEnabled(false); ++z; ar[z]=3;

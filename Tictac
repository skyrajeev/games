class tic extends Frame implements ActionListener
{
 static boolean cond=true; static int u=15000;
 private int ar[]=new int[9];
 private Frame f;
 private Panel p1,p2,p3;
 private String ss="X",s1,s2,s3,s4,s5,s6,s7,s8,s9,user1,user2;
 private int i,z=0;

 private Label ll,l1,l2,tm;
 private TextField tf1,tf2;
 
// f.setDefaultCloseOperation(Frame.EXIT_ON_CLOSE);
 time t=new time();
 Thread th=new Thread(t);
 
 tic()
 {
   f=new Frame(" Tic-Tac_Basic Game ");

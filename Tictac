  class time implements Runnable
 {
            
      public void strt(boolean cnd)
      {
        cond=cnd;u=15000;
      }
      

        @Override
   public void run()
     {  
     
       tm.setVisible(true);
              
       do
       {if(u==0)
           u=15000;
        tm.setText(String.valueOf((u/1000)+" secs left"));
        if(u>10000)
                tm.setBackground(Color.green);
        else if(u >5000)
            tm.setBackground(Color.yellow);
        else if(u>3000)
            tm.setBackground(Color.red);

        u=u-500;
        try{Thread.sleep(500);}catch(Exception eeee){  /* ignore*/}
        
       
        if(!cond || u==0)
          {
             try{
                 
                    tm.setVisible(false);
                    if(u==0)
                          {
                              p2.setVisible(false);
                            p3.setVisible(true);
                            ll.setText("Computer won , Time is up!");
                    
                          }
                     
                    do
                    {Thread.sleep(500); }while(!cond || u==0);
                                       
                 
                     tm.setVisible(true);
                   } catch (Exception ex) {/*ignore*/ }
            
          }   // if cond ends---
      
      }while(cond );    //while loops ends here....
         
    // }//synchronised ends here---
    }//run ends---------------------

 }//time ends here...

/*
 |___________________________________________Constructor ends here____________________________________________________|
